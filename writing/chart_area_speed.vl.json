{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Speed vs size",
  "data": {
    "values": [
      {
        "Compiler": "C++ clang",
        "opt": "size",
        "id": "min_max_cpp_clang_size",
        "Setup": "C++ clang -Os"
      },
      {
        "Compiler": "C++ clang",
        "opt": "speed",
        "id": "min_max_cpp_clang_speed",
        "Setup": "C++ clang -O3"
      },
      {
        "Compiler": "C++ gcc",
        "opt": "size",
        "id": "min_max_cpp_gcc_size",
        "Setup": "C++ gcc -Os"
      },
      {
        "Compiler": "C++ gcc",
        "opt": "speed",
        "id": "min_max_cpp_gcc_speed",
        "Setup": "C++ gcc -O3"
      },
      {
        "Compiler": "Rust",
        "opt": "size",
        "id": "min_max_rust_size",
        "Setup": "Rust -Os"
      },
      {
        "Compiler": "Rust",
        "opt": "speed",
        "id": "min_max_rust_speed",
        "Setup": "Rust -O3"
      },
      {
        "Compiler": "Idiomatic Rust",
        "opt": "size",
        "id": "min_max_rust_idiomatic_size",
        "Setup": "Idiomatic Rust -Os"
      },
      {
        "Compiler": "Idiomatic Rust",
        "opt": "speed",
        "id": "min_max_rust_idiomatic_speed",
        "Setup": "Idiomatic Rust -O3"
      }
    ]
  },
  "transform": [
    {
      "lookup": "id",
      "as": "Estimated area",
      "from": {
        "data": {
          "url": "results/total_estimated_area.csv"
        },
        "key": "id",
        "fields": [
          "value"
        ]
      }
    },
    {
      "lookup": "id",
      "as": "Average cycles",
      "from": {
        "data": {
          "url": "results/average_cycles.csv"
        },
        "key": "id",
        "fields": [
          "value"
        ]
      }
    }
  ],
  "mark": "point",
  "width": 300,
  "height": 165,
  "encoding": {
    "x": {
      "field": "Estimated area",
      "type": "quantitative"
    },
    "y": {
      "field": "Average cycles",
      "type": "quantitative",
      "scale": {
        "domainMin": 9,
        "domainMax": 13
      }
    },
    "color": {
      "field": "Compiler",
      "type": "nominal"
    },
    "shape": {
      "field": "opt",
      "title": "Optimized for",
      "type": "nominal"
    }
  }
}
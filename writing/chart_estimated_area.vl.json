{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Estimated area",
  "data": {
    "values": [
      {
        "Compiler": "C++ clang",
        "opt": "size",
        "id": "min_max_cpp_clang_size",
        "Setup": "C++ clang -Os"
      },
      {
        "Compiler": "C++ clang",
        "opt": "speed",
        "id": "min_max_cpp_clang_speed",
        "Setup": "C++ clang -O3"
      },
      {
        "Compiler": "C++ gcc",
        "opt": "size",
        "id": "min_max_cpp_gcc_size",
        "Setup": "C++ gcc -Os"
      },
      {
        "Compiler": "C++ gcc",
        "opt": "speed",
        "id": "min_max_cpp_gcc_speed",
        "Setup": "C++ gcc -O3"
      },
      {
        "Compiler": "Rust",
        "opt": "size",
        "id": "min_max_rust_size",
        "Setup": "Rust -Os"
      },
      {
        "Compiler": "Rust",
        "opt": "speed",
        "id": "min_max_rust_speed",
        "Setup": "Rust -O3"
      },
      {
        "Compiler": "Idiomatic Rust",
        "opt": "size",
        "id": "min_max_rust_idiomatic_size",
        "Setup": "Idiomatic Rust -Os"
      },
      {
        "Compiler": "Idiomatic Rust",
        "opt": "speed",
        "id": "min_max_rust_idiomatic_speed",
        "Setup": "Idiomatic Rust -O3"
      }
    ]
  },
  "transform": [
    {
      "lookup": "id",
      "as": "Estimated area",
      "from": {
        "data": {
          "url": "results/total_estimated_area.csv"
        },
        "key": "id",
        "fields": [
          "value"
        ]
      }
    }
  ],
  "width": 300,
  "height": 165,
  "encoding": {
    "x": {
      "field": "Compiler",
      "type": "nominal",
      "axis": {
        "labelAngle": 45
      },
      "title": null,
      "scale": {
        "padding": 0.6
      }
    },
    "y": {
      "field": "opt",
      "type": "nominal",
      "title": "Optimized for",
      "scale": {
        "padding": 0.55
      }
    }
  },
  "layer": [
    {
      "mark": "circle",
      "encoding": {
        "size": {
          "field": "Estimated area",
          "type": "quantitative",
          "legend": null,
          "scale": {
            "range": [
              0,
              5000
            ]
          }
        },
        "color": {
          "field": "Compiler",
          "legend": null
        }
      }
    },
    {
      "mark": "text",
      "encoding": {
        "text": {
          "field": "Estimated area",
          "type": "quantitative"
        }
      }
    }
  ]
}